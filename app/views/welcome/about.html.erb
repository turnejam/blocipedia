<div class:'col-md-4'>
<% if current_user.member? %>
<%= button_to "Upgrade my account", charges_path, class: 'btn btn-success' %>
<% elsif current_user.premium? %>
  <%= button_to "Downgrade Your Account", users_update_path, method: :post, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to downgrade?'} %>
<% end %>
</div>
