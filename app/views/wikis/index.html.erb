<h1>Wikis</h2>

  <div class="row">
    <div class="col-md-8">
      <h2> Wikis </h4>
        <% @wikis.each do |wiki| %>
        <h4> <%= link_to wiki.title, wiki %> </h4>
        <%end %>
        </div>
        <% if policy(Wiki).create? %>
        <div class='col-md-4'>
          <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
        </div>
        <% end %>
      </div>
